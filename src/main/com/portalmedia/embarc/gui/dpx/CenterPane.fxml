<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane
	fx:id="borderPane"
	xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="com.portalmedia.embarc.gui.dpx.CenterPaneController"
>
	<top>
		<TabPane
			fx:id="tabPane"
			prefHeight="33.0"
			tabClosingPolicy="UNAVAILABLE"
		>
			<tabs>
				<Tab
					fx:id="generalTab"
					styleClass="general-tab"
					text="General"
				/>
				<Tab
					fx:id="fileInformationTab"
					styleClass="file-info-tab"
					text="File Info"
				/>
				<Tab
					fx:id="imageInformationTab"
					styleClass="img-info-tab"
					text="Image Info"
				/>
				<Tab
					fx:id="imageSourceInformationTab"
					styleClass="img-src-tab"
					text="Image Source Info"
				/>
				<Tab
					fx:id="motionPictureFilmTab"
					styleClass="film-tab"
					text="Motion Picture Film Info"
				/>
				<Tab
					fx:id="televisionTab"
					styleClass="tv-tab"
					text="Television Info"
				/>
				<Tab
					fx:id="userDefinedTab"
					styleClass="user-tab"
					text="User Defined Info"
				/>
			</tabs>
		</TabPane>
	</top>
	<center>
		<TableView fx:id="table" />
	</center>
</BorderPane>
